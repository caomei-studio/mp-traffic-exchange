<view
    class="ad trans"
    bindtap="handleTap"
    data-app-id="{{ apps[current].appId }}"
    wx:if="{{ apps.length > 0 }}"
    style="bottom: {{ bottom }}; backgroud-color: {{ backgroundColor }}"
>
    <view class="ad__avatar" style="background-image: url({{ apps[current].avatarUrl }});"></view>
    <view class="ad__name">{{ apps[current].appName }}</view>
</view>
